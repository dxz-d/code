package demo5;

public class BinaryTree {

	TreeNode root;
	
	//创建根节点
	public void setRoot(TreeNode root) {
		this.root = root;
	}
	
	//获取根节点
	public TreeNode getRoot() {
		return root;
	}
	
	public void frontShow() {
		root.frontShow();
	}
	
	public void midShow() {
		root.midShow();
	}
	
	public void afterShow() {
		root.afterShow();
	}

	/*public void fronSearch(int i) {
		
